<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/white" />
            <corners
                android:bottomLeftRadius="0px"
                android:bottomRightRadius="0px"
                android:topLeftRadius="1000px"
                android:topRightRadius="1000px" />
            <stroke android:width="1dp" android:color="#eee"/>
        </shape>
    </item>
    <item android:top="16dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/white" />

            <corners
                android:bottomLeftRadius="0px"
                android:bottomRightRadius="0px"
                android:topLeftRadius="0px"
                android:topRightRadius="0px" />
        </shape>
    </item>


</layer-list>


