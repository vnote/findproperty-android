<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/etHintColor"/>
            <corners android:radius="99dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/etHintColor"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="@color/etHintColor"/>
            <corners android:radius="99dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/grayLine"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="99dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/appBaseColor"/>
        </shape>
    </item>
</selector>